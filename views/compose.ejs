<%-include("partials/header")-%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" id="postTitle" placeholder="Enter Title">
  </div>
  <div class="form-group">
   <label for="postContent">Post</label>
   <textarea class="form-control" id="postContent" name="postContent" rows="5"></textarea>
 </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>

</form>
<%-include("partials/footer")-%>
